{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@heroicons/*": ["./node_modules/@heroicons/*"],
      "@/*": ["./*"]
    },
    "forceConsistentCasingInFileNames": true
  },"typeRoots": ["node_modules/@types", "node_modules/@heroicons"],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "no.tfggfs"
  ],
  "exclude": ["node_modules"]
}
